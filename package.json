        "sequelize": "^6.6.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/node": "^7.14.7",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "@babel/register": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "chai": "^4.3.4",
        "coveralls": "^3.1.0",
        "eslint": "^7.29.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
